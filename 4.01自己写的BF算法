#include<stdio.h>
int main()
{
	char arr1[20] = "this is a explame";
	char arr2[3] = "exp";
	char *p = arr1; 
	char *q = arr2;
	int i = 0;
	int j = 0;
	int count = 0;

	for(i = 0;i<20;i++)
	{
		if(*p == *q)
		{
			for(j = 1;j<3;j++)
			{
				if(*(p++) == *(q++))
				{
				count++;
				}
			}
			if(count == 3)
			{
				printf("%d ",i-3);
			}
		}
		else
		{
			p = p + 1;
		}
	 } 
	return 0;
 }
