#include<stdio.h>
#include<stdlib.h>

typedef int Elemtype;
typedef struct DualNode
{
	Elemtype data;
	struct DualNode *prior;
	struct DualNode *next;
	
}DualNode,*DualLinkList;

DualLinkList InitLinkList(DualLinkList L)
{
	DualLinkList q,p;
	int i = 0;
	L = (DualLinkList)malloc(sizeof(DualNode));
	if(!(L))
	{
		return L;
	}
	L->next = L->prior = NULL;
	p = L;
	for(i = 1;i<=26;i++)
	{
		q = (DualLinkList)malloc(sizeof(DualNode));
		q->data = i;
		q->prior = p;
		q->next = p->next;
		p->next = q;
		p = q;
	}
	L->next->prior = p;
	p->next = L->next;
	return L; 
}

DualLinkList sort(DualLinkList L,int i)
{
	if(i>0)
	{
		do
		{
			L = L->next;
		}while(--i);
	}
	if(i<0)
	{
		do
		{
			L = L->prior; 
		}while(++i);
	}
	return L;
}
int main()
{
	DualLinkList L;
	int i,n;
	DualLinkList s = InitLinkList(L);
	printf("请输入一个数字:");
	scanf("%d",&n);
	printf("\n");
	s = sort(s,n);
	for(i = 0;i<26;i++)
	{
		s = s->next;
		printf("%d ",s->data);
	}
	return 0;
}
